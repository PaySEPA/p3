{
  "name": "payswarm-auth",
  "version": "0.15.0-rc.11",
  "private": true,
  "scripts": {
    "start": "node payswarm.dev.js",
    "compile-css": "grunt compile-css",
    "ci-test": "node ./node_modules/bedrock/test.js --backend --reporter tap --config `pwd`/configs/test.js",
    "ci-lint": "grunt --no-color jshint --mode=ci || exit 0",
    "coverage": "rm -rf coverage && ./node_modules/.bin/istanbul cover ./node_modules/bedrock/test.js -- --backend --reporter tap --config `pwd`/configs/test.js && ./node_modules/.bin/istanbul report --root coverage lcov cobertura",
    "coverage-backend": "rm -rf coverage && ./node_modules/.bin/istanbul cover ./node_modules/bedrock/test.js -- --backend --config `pwd`/configs/test.js && ./node_modules/.bin/istanbul report --root coverage lcov",
    "coverage-frontend": "rm -rf coverage && ./node_modules/.bin/istanbul cover ./node_modules/bedrock/test.js -- --frontend --config `pwd`/configs/test.js && ./node_modules/.bin/istanbul report --root coverage lcov",
    "cube-collector": "node node_modules/cube/bin/collector.js",
    "cube-evaluator": "node node_modules/cube/bin/evaluator.js",
    "test": "node ./node_modules/bedrock/test.js --backend --frontend --hide --config `pwd`/configs/test.js",
    "test-backend": "node ./node_modules/bedrock/test.js --backend --config `pwd`/configs/test.js",
    "test-frontend": "node ./node_modules/bedrock/test.js --frontend --config `pwd`/configs/test.js",
    "test-frontend-hide": "node ./node_modules/bedrock/test.js --frontend --hide --config `pwd`/configs/test.js",
    "test-chrome": "node ./node_modules/bedrock/test.js --frontend --browser chrome --config `pwd`/configs/test.js",
    "test-chrome-hide": "node ./node_modules/bedrock/test.js --frontend --browser chrome --hide --config `pwd`/configs/test.js",
    "test-firefox": "node ./node_modules/bedrock/test.js --frontend --browser firefox --config `pwd`/configs/test.js",
    "test-firefox-hide": "node ./node_modules/bedrock/test.js --frontend --browser firefox --hide --config `pwd`/configs/test.js",
    "install-protractor": "./node_modules/bedrock/node_modules/.bin/webdriver-manager update",
    "lint": "grunt jshint",
    "minify": "grunt"
  },
  "dependencies": {
    "async": "~0.9.0",
    "bcrypt": "~0.7.8",
    "bedrock": "~0.2.0",
    "bignumber.js": "~1.4.1",
    "bossgeo": "~0.0.7",
    "commander": "~2.3.0",
    "filesaver.js": "~0.1.0",
    "grunt": "~0.4.2",
    "grunt-angular-templates": "git://github.com/digitalbazaar/grunt-angular-templates",
    "grunt-cli": "~0.1.9",
    "grunt-contrib-cssmin": "~0.10.0",
    "grunt-contrib-requirejs": "~0.4.1",
    "jsdom": "~0.7.0",
    "ng-annotate": "~0.10.2",
    "progress": "~1.1.8",
    "request": "~2.45.0",
    "requirejs": "~2.1.8",
    "soap": "git://github.com/digitalbazaar/node-soap.git",
    "sprintf": "~0.1.1",
    "underscore": "~1.7.0",
    "ursa": "v0.8.0"
  },
  "devDependencies": {
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-contrib-less": "~0.11.4",
    "istanbul": "git://github.com/digitalbazaar/istanbul#include-pid",
    "json-schema": "git://github.com/digitalbazaar/json-schema.git",
    "payswarm": "~0.10.6",
    "pkginfo": "~0.3.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "main": "payswarm.dev"
}
