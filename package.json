{
  "name": "payswarm-auth",
  "version": "0.14.27",
  "private": true,
  "scripts": {
    "start": "node payswarm.dev.js",
    "bootstrap": "make -C ../payswarm-bootstrap bootstrap && rsync -av ../payswarm-bootstrap/bootstrap/ ./site/static/bootstrap/",
    "ci-test": "node test.js --backend --frontend",
    "ci-lint": "grunt jshint --mode=ci || exit 0",
    "coverage": "mkdir -p reports && rm -rf lib-cov; ./node_modules/visionmedia-jscoverage/jscoverage lib lib-cov && BEDROCK_COV=1 node test.js --backend > reports/coverage.html && rm -rf lib-cov",
    "cube-collector": "node node_modules/cube/bin/collector.js",
    "cube-evaluator": "node node_modules/cube/bin/evaluator.js",
    "test": "node ./node_modules/bedrock/test.js --backend --frontend --hide --config `pwd`/configs/test.js",
    "test-backend": "node ./node_modules/bedrock/test.js --backend --config `pwd`/configs/test.js",
    "test-frontend": "node ./node_modules/bedrock/test.js --frontend --config `pwd`/configs/test.js",
    "test-frontend-hide": "node ./node_modules/bedrock/test.js --frontend --hide --config `pwd`/configs/test.js",
    "test-chrome": "node ./node_modules/bedrock/test.js --frontend --browser chrome --config `pwd`/configs/test.js",
    "test-chrome-hide": "node ./node_modules/bedrock/test.js --frontend --browser chrome --hide --config `pwd`/configs/test.js",
    "test-firefox": "node ./node_modules/bedrock/test.js --frontend --browser firefox --config `pwd`/configs/test.js",
    "test-firefox-hide": "node ./node_modules/bedrock/test.js --frontend --browser firefox --hide --config `pwd`/configs/test.js",
    "install-protractor": "./node_modules/bedrock/node_modules/.bin/webdriver-manager update",
    "lint": "grunt jshint",
    "minify": "grunt"
  },
  "dependencies": {
    "async": "~0.2.8",
    "almond": "~0.2.6",
    "bcrypt": "~0.7.2",
    "bignumber.js": "~1.2.1",
    "bossgeo": "~0.0.7",
    "commander": "~1.3.0",
    "connect": "~2.7.10",
    "connect-mongo": "git://github.com/digitalbazaar/connect-mongo.git",
    "cube": "~0.2.0",
    "cubism": "^1.6.0",
    "d3": "^3.4.8",
    "emailjs": "git://github.com/digitalbazaar/emailjs.git",
    "express": "~3.3.5",
    "filesaver.js": "^0.1.0",
    "grunt": "~0.4.2",
    "grunt-angular-templates": "git://github.com/digitalbazaar/grunt-angular-templates",
    "grunt-cli": "~0.1.9",
    "grunt-contrib-cssmin": "~0.7.0",
    "grunt-contrib-requirejs": "~0.4.1",
    "http-signature": "~0.10.0",
    "i18n": "~0.4.0",
    "jsdom": "~0.7.0",
    "json-schema": "git://github.com/digitalbazaar/json-schema.git",
    "jsonld": "~0.1.25",
    "mailparser": "~0.3.0",
    "mongodb": "~1.3.18",
    "passport": "~0.1.9",
    "passport-local": "~0.1.3",
    "pkginfo": "~0.3.0",
    "posix": "~1.0.0",
    "prompt": "~0.2.11",
    "redback": "~0.3.6",
    "request": "~2.27.0",
    "requirejs": "~2.1.8",
    "soap": "git://github.com/digitalbazaar/node-soap.git",
    "sprintf": "~0.1.1",
    "swig": "git://github.com/digitalbazaar/swig.git",
    "ua-parser": "~0.3.0",
    "underscore": "^1.6.0",
    "ursa": "v0.8.0",
    "walk": "~2.2.1",
    "winston": "~0.7.0",
    "winston-mail": "~0.2.1"
  },
  "devDependencies": {
    "grunt-contrib-jshint": "^0.10.0",
    "mocha": "~1.12.0",
    "payswarm": "~0.10.6",
    "should": "~1.2.2",
    "superagent": "*",
    "sv-selenium": "*",
    "visionmedia-jscoverage": "~1.0.0",
    "wd": "*"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "directories": {
    "lib": "./lib/payswarm-auth"
  },
  "main": "./lib/payswarm-auth"
}
